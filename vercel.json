{
    "version": 2,
    "rewrites": [
        { 
            "source": "/(.*)", 
            "destination": "/" 
        }
    ],
    "builds": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.js"
        }
    ]
}